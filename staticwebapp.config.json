{
  "routes": [
    {
      "route": "/maintenance/*",
      "rewrite": "/maintenance/index.html" // statusCode: 200 を削除
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/*",
      "/assets/*",
      "*.{png,jpg,jpeg,gif,css,js,ico,xml,json,webmanifest}",
      "/maintenance"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html" // statusCode: 200 を削除
    }
  }
}