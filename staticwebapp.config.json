{
  "routes": [
    {
      "route": "/maintenance/static/*",
      "rewrite": "/static/{*}" 
    },
    {
      "route": "/maintenance/manifest.json",
      "rewrite": "/manifest.json"
    },
    {
      "route": "/maintenance/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}